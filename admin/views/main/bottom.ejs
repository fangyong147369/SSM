<script>
    var txturl = window.location.pathname;
    $(document).ready(function () {
        App.init();
        $(".nav_all li").each(function () {
            if ($(this).children("a").attr("_attr") == txturl) {
                $(this).addClass("active");
                $(this).parent().parent().addClass("active");
                $(this).parent().parent().children().children(".arrow").addClass("open")
            }
        });
    });
</script>
